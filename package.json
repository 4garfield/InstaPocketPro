{
  "name": "instapocketpro",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:4garfield/InstaPocketPro.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "cleanDist": "node ./scripts/cleanDist",
    "build:ssr": "npm run cleanDist && npm run build:ssr:browser && npm run build:ssr:server && npm run webpack:server",
    "build:ssr:browser": "ng build --prod --app browser --vendor-chunk true --common-chunk true --output-hashing none --extract-licenses false",
    "build:ssr:server": "ng build --prod --app server --output-hashing none --extract-licenses false",
    "webpack:server": "webpack --config ssr/webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/app.js",
    "build:aws": "npm run cleanDist && npm run build:aws:browser && npm run build:aws:server && npm run webpack:server",
    "build:aws:browser": "ng build --prod --app browser --base-href /production/ --vendor-chunk true --common-chunk true --output-hashing none --extract-licenses false",
    "build:aws:server": "ng build --prod --app server --base-href /production/ --output-hashing none --extract-licenses false",
    "deploy": "npm run build:aws && serverless deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --no-webdriver-update"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/cdk": "5.2.5",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/flex-layout": "5.0.0-beta.15",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/material": "5.2.5",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@angular/platform-server": "5.2.11",
    "@angular/router": "5.2.11",
    "@angular/service-worker": "5.2.11",
    "@nguniversal/express-engine": "5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0",
    "@types/aws-serverless-express": "^3.3.1",
    "aws-serverless-express": "^3.3.6",
    "core-js": "2.6.9",
    "hammerjs": "2.0.8",
    "mobx": "4.12.0",
    "mobx-angular": "3.0.1",
    "rxjs": "5.5.12",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.11",
    "@angular/language-service": "5.2.11",
    "@types/chai": "4.1.7",
    "@types/cucumber": "4.0.6",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "8.10.50",
    "chai": "4.2.0",
    "codelyzer": "4.5.0",
    "cucumber": "5.1.0",
    "fs-extra": "^8.1.0",
    "jasmine-core": "2.9.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "6.0.0",
    "protractor-cucumber-framework": "6.1.2",
    "serverless": "^1.46.1",
    "ts-loader": "3.5.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  }
}
