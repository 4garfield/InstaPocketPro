{
  "name": "instapocketpro",
  "version": "5.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:4garfield/instapocketpro.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --dev",
    "clean": "node ./scripts/clean",
    "copy": "node ./scripts/copy",
    "build:ssr": "npm run clean && npm run build:ssr:browser && npm run build:ssr:server && npm run webpack:server",
    "build:ssr:browser": "ng build --prod --app browser --vendor-chunk true --common-chunk true",
    "build:ssr:server": "ng build --prod --app server --output-hashing none --extract-licenses false",
    "webpack:server": "webpack --config ssr/webpack.server.config.js --progress --colors",
    "serve:ssr": "cd dist && node local.js",
    "pre-firebase": "cd dist && npm install && cd ../",
    "build:firebase": "npm run build:ssr && npm run copy && npm run pre-firebase",
    "serve:firebase": "firebase serve",
    "deploy:firebase": "firebase deploy",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e --no-webdriver-update"
  },
